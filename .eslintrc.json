{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "prettier/prettier": 1,
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  },
  "parser": "@typescript-eslint/parser"
}
